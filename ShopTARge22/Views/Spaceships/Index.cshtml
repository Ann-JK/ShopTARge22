@model IEnumerable<ShopTARge22.Models.Spaceships.SpaceshipsIndexViewModel>

<div class="h1">
    Products
</div>
<form method="post">
    <table class="table table-hover">
        <thead>
            <th scope="col">Nr</th>
            <th scope="col">Name</th>
            <th scope="col">Type</th>
            <th scope="col">Built date</th>
            <th scope="col">Passengers</th>
        </thead>
        <tbody>
            @foreach (var spaceships in Model)
            {
                <tr>
                    <td>spaceships.Name</td>
                    <td>spaceships.Type</td>
                    <td>spaceships.BuiltDate</td>
                    <td>spaceships.Passengers</td>
                </tr>
            }
        </tbody>
    </table>
</form>